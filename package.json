{
  "name": "gitshook",
  "version": "1.0.0",
  "description": "Webhook server to sync remote repositories",
  "homepage": "https://github.com/thyares/gitshook",
  "main": "index.js",
  "scripts": {
    "ci": "yarn install --production --frozen-lockfile",
    "dev": "yarn install && nodemon src",
    "start": "node src",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "lint:format": "prettier --write '**/*.{ts,tsx,js,jsx,json}'"
  },
  "keywords": [
    "git",
    "sync",
    "gitsync",
    "webhook"
  ],
  "author": "Charles Santos <thyarles@gmail.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "express": "^4.18.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "eslint": "^8.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "husky": "^8.0.0"
  },
  "repository": "https://github.com/thyarles/gitshook.git",
  "private": true
}
